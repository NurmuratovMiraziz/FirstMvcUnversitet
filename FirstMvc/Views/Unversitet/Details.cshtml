@model FirstMvc.Models.Unversitet

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Ma'lumotlar</h1>

<div>
    <h4>Unversitet</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Talabas)
        </dt>
        <dd class = "col-sm-10">
            @foreach(var item in Model.Talabas.ToList())
            {
                <br/>
                @Html.DisplayFor(modelItem => item.Ismi)
            }
        </dd>
        
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
